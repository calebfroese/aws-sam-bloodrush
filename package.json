  "bugs": {
    "url": "https://github.com/calebfroese/aws-sam-bloodrush/issues"
  },
  "homepage": "https://github.com/calebfroese/aws-sam-bloodrush#readme",
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.11.3",
    "@types/sinon": "^5.0.2",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.4",
    "sinon": "^6.3.4"
  },
  "dependencies": {
    "aws-sdk": "^2.325.0",
    "rxjs": "^6.3.3"
  }
